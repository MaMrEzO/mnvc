{
	"codeLens.enable": true,
	"diagnostic.virtualText": false,
	"diagnostic.errorSign": " ",
	"diagnostic.warningSign": " ",
	"diagnostic.infoSign": " ",
	"diagnostic.enableMessage": "always",
	"diagnostic.enableHighlightLineNumber": true,
	"diagnostic.hintSign": ">>",
	"signature.preferShownAbove": false,
	"coc.preferences.jumpCommand": "drop",
	"coc.preferences.formatOnSaveFiletypes": ["go", "js", "ts", "rust", "vim", "dart"],
	"suggest.enablePreview": false,
	"suggest.detailField": "preview",
	"suggest.noselect": true,
	"suggest.removeDuplicateItems": true,
	"suggest.numberSelect": false,
	"rust-analyzer.callInfo.full": true,
	"languageserver": {
		"go": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"trace.server": "verbose",
			"filetypes": ["go"],
			"args":	["-remote=auto"],
			"formatterPriority": 99
		}
	},
	"suggest.completionItemKindLabels": {
		"function": "",
		"method": " ",
		"variable": "ⅉ",
		"constant": "",
		"struct": " ",
		"class": "",
		"interface": "",
		"text": "",
		"enum": "",
		"enumMember": "",
		"module": " ",
		"color": "",
		"property": " ",
		"field": " ",
		"unit": " ",
		"file": "",
		"value": "\uf8a3",
		"event": "\ufacd",
		"folder": "",
		"keyword": "",
		"snippet": "\uf64d",
		"operator": "\uf915",
		"reference": "\uf87a",
		"typeParameter": "\uf278",
		"default": "\uf29c"
	},
	"suggest.languageSourcePriority": 99,
	"suggest.defaultSortMethod": "alphabetical",
	"coc.source.around.priority": 98,
	"suggest.detailMaxLength": 0,
	"rust.unstable_features": true,
	"rust.show_hover_context": true,
	"rust.racer_completion": true,
	"rust-analyzer.hoverActions.enable": true,
	"rust-analyzer.lens.enable": true,
	"rust-analyzer.lens.methodReferences": true,
	"flutter.trace.server": "verbose",
	"flutter.autoOpenDevLog": true,
	"flutter.UIPath": false,
	"flutter.lsp.initialization.outline": true,
	"flutter.autoHideDevLog": false,
	"sql.database": "postgresql",
	"cSpell.allowCompoundWords": true,
	"cSpellExt.enableDictionaries": [
		"farsi",
		"persian",
		"medical-terms"
	],
	"cSpell.language": "en,fa",
	"cSpell.userWords": [
		"Unprocessable",
		"actix",
		"derror",
		"dotenv",
		"ilike",
		"strs",
		"undelete",
		"undeleted",
		"باشد",
		"باید",
		"جدید",
		"حداقل",
		"کاراکتر",
		"گذرواژه"
	],
	"explorer.icon.enableNerdfont": true,
	"explorer.icon.collapsed": "⏷",
	"explorer.icon.expanded": "⏵",
	"explorer.floating.position": "center",
	"git.virtualTextPrefix": "git",
	"git.addGBlameToVirtualText": true,
	"git.enableGlobalStatus": true,
	"git.addedSign.hlGroup": "GitGutterAdd",
	"git.changedSign.hlGroup": "GitGutterChange",
	"git.removedSign.hlGroup": "GitGutterDelete",
	"git.topRemovedSign.hlGroup": "GitGutterDelete",
	"git.untrackedDecorator": "…",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.enableGutters": true

}
